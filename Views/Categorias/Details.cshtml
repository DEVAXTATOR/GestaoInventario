@model GestaoInventario.Models.Categoria

@{
    ViewData["Title"] = "Detalhes da Categoria";
    Layout = "_Layout";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Categoria</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nome:
        </dt>
        <dd class="col-sm-10">
            @Model.Nome
        </dd>
    </dl>
</div>
<button onclick="location.href='@Url.Action("Index", "Categorias")'" class="btn btn-primary">Voltar</button>
